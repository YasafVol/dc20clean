{
	"outputDirectory": "dist",
	"headers": [
		{
			"source": "/assets/(.*)\\.js",
			"headers": [
				{ "key": "Content-Type", "value": "application/javascript; charset=utf-8" }
			]
		}
	],
	"routes": [
		{
			"src": "/assets/(.*)",
			"dest": "/assets/$1"
		},
		{
			"src": "/static/(.*)",
			"dest": "/$1"
		},
		{
			"src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
			"dest": "/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
